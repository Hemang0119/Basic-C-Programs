#include <stdio.h>

int main(void) {
  int i,n,sum=0;
  printf("Enter number of integers=");
  scanf("%d",&n);
  int a[100];
  for(i=0;i<n;++i)
  {
    printf("\nEnter value=");
    scanf("%d",&a[i]);
  }
  for(i=0;i<n;++i)
  {
    sum+=a[i];
  }
  printf("\nSum of numbers is=");
  printf("%d",sum);
  return 0;
}
