#include <stdio.h>

void get(int x[100])
{
  int i,n;
  printf("Enter number of integers=");
  scanf("%d",&n);
  for(i=0;i<n;++i)
  {
    printf("\nEnter value=");
    scanf("%d",&x[i]);
  }
  printf("the array is=");
  
  for(i=0;i<n;++i)
  {
    printf(" %d",x[i]);
  }
}

int main(void) {
  int a[100];
  get(a);
  return 0;
}
