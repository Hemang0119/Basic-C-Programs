#include <stdio.h>

int main(void) {
  float a,b,c,s;
  printf("Enter value of a =");
  scanf("%f",&a);
  printf("Enter value of b =");
  scanf("%f",&b);
  printf("Enter value of c =");
  scanf("%f",&c);
  if(a>b)
  {
    if(a>c)
    {
      s=a;
    }
  }
  else if(b>a)
  {
    if(b>c)
    {
      s=b;
    }
  }
  else
  s=c;
  printf("\nGreatest number=%f",s);
  return 0;
}
