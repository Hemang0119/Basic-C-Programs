#include <stdio.h>

int main(void) {
  int i,n;
  float sum=0;
  float a[100];
  printf("Enter number of integers=");
  scanf("%d",&n);
  for(i=0;i<n;++i)
  {
    printf("\nEnter value=");
    scanf("%f",&a[i]);
    a[i]=sqrt(a[i]);
  }
  
  for(i=0;i<n;++i)
  {
    sum+=a[i];
  }
  printf("Sum of roots=%f",sum);
  return 0;
}
